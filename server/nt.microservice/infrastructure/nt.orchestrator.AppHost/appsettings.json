{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "InfrastructureSettings": {
    "ApplicationRoot": "D:\\Source\\nt\\server\\nt.microservice",
    "Consul": {
      "DockerImage": "hashicorp/consul:latest",
      "HostPort": 9500,
      "TargetPort": 8500
    },
    "RabbitMq": {
      "Host": "localhost",
      "UserName": "ntuser",
      "Password": "pass",
      "HttpPort": 5672,
      "HttpsPort": 15671
    },
    "Postgres": {
      "DockerImage": "postgres:14.1-alpine",
      "UserName": "postgres",
      "Password": "Admin123"
    },
    "MongoDb": {
      "UserName": "root",
      "Password": "mypass"
    },
    "BlobStorage": {
      "DockerImage": "mcr.microsoft.com/azure-storage/azurite",
      "HostPort": 10000,
      "TargetPort": 10000
    },

    "SqlServer": {
      "Password": "Admin123",
      "HostPort": 1433,
      "TargetPort": 1433
    }
  },
  "ServicesSettings": {
    "Gateway": {
      "Host": "localhost"
    },
    "AggregateAuthUserService": {
      "ServiceDiscovery": {
        "ServiceName": "nt.useridentityaggregator.service",
        "ServiceId": "useridentityaggregator-1",
        "ServiceAddress": "localhost",
        "ServicePort": 8301,
        "HealthCheckUrl": "/api/healthcheck/health",
        "DeregisterAfterMinutes": 5
      },
      "ServiceDiscoveryOptions": {
        "ResolverName": "localhost",
        "ResolverPort": 8500,
        "Services": [
          {
            "Key": "UserService",
            "Name": "nt.userservice.service"
          },
          {
            "Key": "AuthService",
            "Name": "nt.authservice.loadbalancer"
          }
        ]
      }
    },
    "AuthService": {
      "InstancePorts": [ 8101, 8102, 8103, 8104, 8105 ],
      "LoadBalancer": {
        "DockerImage": "nginx:latest",
        "HostPort": 8100,
        "TargetPort": 80
      },
      "ConsulSideCar": {
        "ServiceName": "nt.authservice.loadbalancer",
        "ServiceId": "authservice-1",
        "ServiceAddress": "localhost",
        "ServicePort": 8100,
        "HealthCheckUrl": "http://host.docker.internal:8100/health",
        "DeregisterAfterMinutes": 5
      }
    },
    "UserService": {
      "ServiceDiscovery": {
        "ServiceName": "nt.userservice.service",
        "ServiceId": "userservice-1",
        "ServiceAddress": "localhost",
        "ServicePort": 8301,
        "HealthCheckUrl": "/api/healthcheck/health",
        "DeregisterAfterMinutes": 5
      }
    },
    "MovieService": {
      "DbName": "ntmoviestore",
      "MovieCollectionName": "movies",
      "ServiceDiscovery": {
        "ServiceName": "nt.movieservice.service",
        "ServiceId": "movieservice-1",
        "ServiceAddress": "localhost",
        "ServicePort": 8301,
        "HealthCheckUrl": "http://host.docker.internal:8301/api/healthcheck/health",
        "DeregisterAfterMinutes": 5
      }
    }
  }
}
